/* components/searchBar/searchBar.wxss */

.searchbar {
    background-color: #4ebe57;
    padding: 20rpx;
}

.search-navigator {
    width: 100%;
    height: 60rpx;
    background: #fff;
    border-radius: 10rpx;
    background-image: url(data:image/png;base64,);
    background-position: center center;
    background-repeat: no-repeat;
    background-size: 4.5%;
}

.search-input-group {
    width: 100%;
    height: 60rpx;
    background: #fff;
    border-radius: 10rpx;
    box-sizing: border-box;
    padding: 10rpx;
}

.search-input {
    min-height: 40rpx;
    height: 40rpx;
    font-size: 12px;
    line-height: 40rpx;
}